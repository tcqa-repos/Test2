<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Tests from Calculator subproject IntelliJ 2017.2</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="**/* =&gt; artifact.zip&#xA;%system.teamcity.build.tempDir%/inspection*result/** =&gt; inspections-reports-data-%build.number%.zip&#xA;%system.teamcity.build.tempDir%/idea-logs/** =&gt; inspections-reports-idea-logs-%build.number%.zip" />
      <option name="shouldFailBuildIfTestsFailed" value="false" />
      <option name="shouldFailBuildOnBadExitCode" value="false" />
    </options>
    <disabled-settings>
      <setting-ref ref="RUNNER_2" />
      <setting-ref ref="RUNNER_3" />
    </disabled-settings>
    <parameters>
      <param name="system.teamcity.dont.delete.temp.result.dir" value="true" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="" type="gradle-runner">
        <parameters>
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="myPackage.*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.coverage.runner" value="IDEA" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRUnner.gradle.build.file" value="" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="clean :Calculator:build" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="New build step" type="gradle-runner">
        <parameters>
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
          <param name="ui.gradleRUnner.gradle.build.file" value="" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="clean :Calculator:build" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="" type="SleepNSeconds">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
          <param name="timeout-in-seconds" value="6000" />
        </parameters>
      </runner>
      <runner id="RUNNER_4" name="" type="Inspection">
        <parameters>
          <param name="idea.app.home" value="%teamcity.tool.intellij.ideaIU-teamcity-2017.2.6%" />
          <param name="includeExclude.patterns"><![CDATA[+:src/**
-:*/build/**]]></param>
          <param name="iprInfoRunParam" value="&lt;iprInfo path-to-ipr=&quot;build.gradle&quot; /&gt;" />
          <param name="jvmArgs" value="-Xmx512m -XX:ReservedCodeCacheSize=240m" />
          <param name="target.jdk.home" value="%env.JDK_18%" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
      <runner id="RUNNER_5" name="" type="Duplicator">
        <parameters>
          <param name="duplicates.runner.bound" value="1" />
          <param name="duplicates.runner.discard" value="0" />
          <param name="duplicates.runner.include.tests" value="true" />
          <param name="duplicates.runner.literal" value="true" />
          <param name="duplicates.runner.method" value="true" />
          <param name="duplicates.runner.type" value="true" />
          <param name="duplicates.runner.visibility" value="true" />
          <param name="idea.app.home" value="%teamcity.tool.intellij.ideaIU-teamcity-2017.2.6%" />
          <param name="iprInfoRunParam" value="&lt;iprInfo path-to-ipr=&quot;build.gradle&quot; /&gt;" />
          <param name="jvmArgs" value="-Xmx1G -XX:ReservedCodeCacheSize=240m" />
          <param name="target.jdk.home" value="%env.JDK_18%" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="CompositeBuilds_MyProject_HttpsGithubComTcqaReposGradleRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

