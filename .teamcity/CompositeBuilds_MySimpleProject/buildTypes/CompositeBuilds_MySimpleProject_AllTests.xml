<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>All tests</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="ARTIFACT_DEPENDENCY_2" />
    </disabled-settings>
    <parameters>
      <param name="teamcity.compositeBuild.artifacts.archiveIntrospection.enabled" value="true" />
    </parameters>
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="BUILD_EXT_1" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="buildTestCount" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="buildFailedTestCount" />
          <param name="metricThreshold" value="0" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_3" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="CodeCoverageAbsCCovered" />
          <param name="metricThreshold" value="10" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="less" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_4" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="TestsDuration" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_5" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="test" />
          <param name="buildFailureOnMessage.outputText" value="failed" />
          <param name="buildFailureOnMessage.reverse" value="true" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_6" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="buildLogSize" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="different" />
          <param name="withBuildAnchor" value="true" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_7" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="InspectionStatsE" />
          <param name="metricThreshold" value="10" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="less" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_8" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="ArtifactsSize" />
          <param name="metricThreshold" value="2MB" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_9" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="BuildDurationNetTime" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="different" />
          <param name="withBuildAnchor" value="true" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_10" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="DuplicatorStats" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="CompositeBuilds_MySimpleProject_NewSubProject_Tests2" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="artifact.zip!/Fetch.bat =&gt;aaa&#xA;artifact.zip!/ANSI codes/** =&gt; aaa&#xA;artifact.zip =&gt; aaa" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_2" sourceBuildTypeId="CompositeBuilds_MySimpleProject_NewSubProject_Tests21" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="artifact.zip" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="CompositeBuilds_MyProject_BuildWithDuplicates">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="CompositeBuilds_MySimpleProject_NewSubProject_Tests2">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="CompositeBuilds_MySimpleProject_NewSubProject_Tests21">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

