<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>All Tests (no artifacts)</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="ARTIFACT_DEPENDENCY_1" />
      <setting-ref ref="ARTIFACT_DEPENDENCY_2" />
    </disabled-settings>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="CompositeBuilds_SubProject_Tests1" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="folder with spaces/**/*" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_2" sourceBuildTypeId="CompositeBuilds_SubProject_Tests2" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="folder with spaces =&gt; folder with spaces" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="CompositeBuilds_SubProject_SubSubProject_Tests2_2">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="CompositeBuilds_SubProject_Tests1">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="CompositeBuilds_SubProject_Tests2">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

