<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>All Tests (1)</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="COMPOSITE" />
      <option name="showDependenciesChanges" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="ARTIFACT_DEPENDENCY_2" />
      <setting-ref ref="BUILD_EXT_1" />
    </disabled-settings>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="retryBuildTrigger">
        <parameters>
          <param name="enqueueTimeout" value="0" />
          <param name="retryAttempts" value="1" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="BuildFailureOnMetric">
        <parameters>
          <param name="anchorBuild" value="lastSuccessful" />
          <param name="metricKey" value="buildTestCount" />
          <param name="metricThreshold" value="1" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="stopBuildOnFailure" value="true" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="aaa" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="CompositeBuilds_SubProject_Tests1" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="artifact.zip" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_2" sourceBuildTypeId="CompositeBuilds_SubProject_Tests2" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="artifact.zip" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="CompositeBuilds_SubProject_Tests1">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="CompositeBuilds_SubProject_Tests2">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

